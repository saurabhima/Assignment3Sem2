init : HTMLTAG head;
HTML_COMMENT : HEADCOMMENT_SECTION+ ->skip;
HEADCOMMENT_SECTION:'<!' .*? '>'|'<!--'.*?'-->' ;
head: HEAD_TAG HEAD_DATA* HEAD_TAG;
