HEAD_DATA:METATAG|LINKTAG|TITLETAG|SCRIPTTAG;
METATAG:STARTENDINDENT 'meta'.*? STARTENDINDENT;
LINKTAG : STARTENDINDENT 'link' .*? STARTENDINDENT;
TITLETAG : STARTENDINDENT 'title'STARTENDINDENT .*? STARTENDINDENT ENDTAG 'title' STARTENDINDENT;
SCRIPTTAG: STARTENDINDENT 'script' .*? STARTENDINDENT|STARTENDINDENT ENDTAG 'script' STARTENDINDENT;
HTMLTAG : STARTENDINDENT 'html' STARTENDINDENT|STARTENDINDENT 'HTML' STARTENDINDENT|STARTENDINDENT ENDTAG'html' STARTENDINDENT |STARTENDINDENT ENDTAG'HTML' STARTENDINDENT |HTMLASSIGNTAG;
HTMLASSIGNTAG :STARTENDINDENT 'html' .*? '='.*? STARTENDINDENT;
HEAD_TAG :STARTENDINDENT 'head' STARTENDINDENT|STARTENDINDENT ENDTAG 'head' STARTENDINDENT;
STARTENDINDENT : '<'|'>';
SPLEXP :'!'|'=';
ENDTAG : '/';
LETTERS :[a-zA-Z]+;
WS : [ \t\r\n]+ -> skip;